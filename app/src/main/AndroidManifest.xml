<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.osan.nebula">

    <application
        android:name="com.osan.nebula.App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.osan.nebula.MainActivity"
            android:theme="@style/MainTheme"></activity>
        <activity
            android:name="com.osan.nebula.FlashActivity"
            android:theme="@style/MainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.osan.nebula.AppFlowActivity"></activity>
    </application>

</manifest>
